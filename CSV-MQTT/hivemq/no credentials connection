Broker: broker.hivemq.com
Port: 1883
TLS: No
Username/Password: None

# Terminal
# Publishing on a test topic a simple msg
mosquitto_pub -h broker.hivemq.com -p 1883 -t "UE/test" -m "lets go baby"
# Subscriing to the same topc to receive msg with verbose (topic + msg)
mosquitto_sub -h broker.hivemq.com -p 1883 -t "UE/test" -v

